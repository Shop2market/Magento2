<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
	<default>
		<adcurve>
			<settings>
				<enabled>1</enabled>
				<test_mode/>
				<url_register_live>https://app.adcurve.com/integrations/magento/install</url_register_live>
				<url_register_test>https://capuchin.shop2market.com/integrations/magento/install</url_register_test>
